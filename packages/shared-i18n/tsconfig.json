{
    "compilerOptions": {
        "jsx": "react-jsx",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": true,
        "module": "CommonJS",
        "moduleResolution": "Node",
        "target": "ES2020",
        "outDir": "dist",
        "rootDir": "src",
        "composite": true,
        "declaration": true
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.json"
    ]
}
