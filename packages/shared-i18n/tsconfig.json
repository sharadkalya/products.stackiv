    "compilerOptions": {
        "jsx": "react-jsx",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES2020",
        "outDir": "dist",
        "composite": true,
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.json"
    ]
}
